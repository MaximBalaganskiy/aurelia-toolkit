{
  "name": "aurelia-toolkit",
  "version": "2.20.1",
  "registry": "github",
  "description": "",
  "author": "Maxim Balaganskiy",
  "license": "ISC",
  "keywords": [
    "aurelia",
    "materialize"
  ],
  "homepage": "https://github.com/MaximBalaganskiy/aurelia-toolkit",
  "bugs": {
    "url": "https://github.com/MaximBalaganskiy/aurelia-toolkit/issues"
  },
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint --project tsconfig.json",
    "pretest": "npm run lint",
    "build:amd": "tsc --outDir dist/amd --module amd",
    "postbuild:amd": "copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss src/**/*.map dist/amd",
    "build:commonjs": "tsc --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss src/**/*.map dist/commonjs",
    "build:es2015": "tsc --outDir dist/es2015 --module es2015",
    "postbuild:es2015": "copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss src/**/*.map dist/es2015",
    "build:native-modules": "tsc --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss src/**/*.map dist/native-modules",
    "build:system": "tsc --outDir dist/system --module system",
    "postbuild:system": "copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss src/**/*.map dist/system",
    "prebuild": "rimraf dist",
    "compile-scss": "sass src/elements/colours/at-colours.scss src/elements/colours/at-colours.css --no-source-map --style=compressed",
    "replace-css": "ts-node ./replace-css.ts",
    "build:colours": "npm run compile-scss & npm run replace-css",
    "build": "concurrently \"npm run build:amd\" \"npm run build:commonjs\" \"npm run build:es2015\" \"npm run build:native-modules\" \"npm run build:system\"",
    "postbuild": "concurrently \"npm run postbuild:amd\" \"npm run postbuild:commonjs\" \"npm run postbuild:es2015\" \"npm run postbuild:native-modules\" \"npm run postbuild:system\"",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximBalaganskiy/aurelia-toolkit"
  },
  "peerDependencies": {
    "@types/numeral": ">=0.0.26",
    "@microsoft/applicationinsights-web": "^2.2.0",
    "aurelia-authentication": ">=3.8.3",
    "aurelia-dependency-injection": "^1.5.0",
    "aurelia-event-aggregator": ">=1.0.3",
    "aurelia-framework": ">=1.3.1",
    "aurelia-i18n": "^3.0.0-beta.7",
    "aurelia-inputmask": "^1.1.2",
    "aurelia-materialize-bridge": ">=1.4.0",
    "aurelia-router": ">=1.7.1",
    "aurelia-validation": ">=1.5.0",
    "inputmask": ">=4.0.8",
    "lodash": ">=4.17.15",
    "materialize-css": ">=1.0.0",
    "tslib": ">=1.10.0",
    "moment": ">=2.24.0",
    "numeral": ">=2.0.6"
  },
  "devDependencies": {
    "@microsoft/applicationinsights-web": "^2.2.0",
    "@types/lodash": ">=4.14.137",
    "@types/node": ">=12.7.2",
    "@types/numeral": ">=0.0.26",
    "aurelia-authentication": ">=3.8.3",
    "aurelia-dependency-injection": "^1.5.0",
    "aurelia-event-aggregator": ">=1.0.3",
    "aurelia-framework": ">=1.3.1",
    "aurelia-i18n": "^3.0.0-beta.7",
    "aurelia-inputmask": "^1.1.2",
    "aurelia-materialize-bridge": "^1.4.0",
    "aurelia-router": ">=1.7.1",
    "aurelia-typed-observable-plugin": ">=0.4.2",
    "aurelia-validation": ">=1.5.0",
    "concurrently": "^4.1.2",
    "copyfiles": ">=2.1.1",
    "inputmask": ">=4.0.8",
    "lodash": "^4.17.15",
    "materialize-css": ">=1.0.0",
    "moment": ">=2.24.0",
    "numeral": ">=2.0.6",
    "replace-in-file": "^4.1.3",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "ts-node": "^8.3.0",
    "tslib": ">=1.10.0",
    "typescript": "^3.5.3"
  }
}
